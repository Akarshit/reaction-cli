    "babel-core": "^6.9.1",
    "babel-eslint": "^6.0.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-root-slash-import": "^1.0.0",
    "eslint": "^2.12.0",
    "eslint-plugin-babel": "^3.2.0"
  },
  "dependencies": {
    "chalk": "^1.1.3",
